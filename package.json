{
  "name": "skee",
  "version": "0.5.6",
  "description": "Fluent schema modeling and migrations, versioning, and codegen",
  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vriad/skee"
  },
  "author": "Colin McDonnell <colin@vriad.com>",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/vriad/skee/issues"
  },
  "homepage": "https://github.com/vriad/skee",
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.4",
    "ts-toolbelt": "^6.6.2"
  },
  "tags": [],
  "keywords": [],
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "**/__tests__/*"
  ],
  "scripts": {
    "clean": "rm -rf lib/*",
    "build": "yarn run clean && tsc --p tsconfig.package.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json --coverage",
    "prepublishOnly": "npm run build",
    "play": "nodemon -e ts -w . -x ts-node src/playground.ts",
    "check": "gts check",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/pg": "^7.14.3",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^7.5.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "pg": "^8.2.1",
    "prettier": "2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3",
    "@types/node": "^13.11.1"
  },
  "peerDependencies": {
    "pg": "^7"
  }
}
